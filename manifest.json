{
  "name": "SLink - Simple copy link",
  "description": "Copy page link and title, so simple!",
  "version": "0.1",
  "icons": {
    "48": "icon.png",
    "96": "icon@2x.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    }
  },
  "permissions": ["contextMenus"],
  "background": {
    "persistent": false,
    "scripts": ["simple-copy-link.js"]
  },
  "manifest_version": 2
}
